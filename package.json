{
  "name": "apicloud-polyfill-sample",
  "version": "0.0.1",
  "description": "apicloud-polyfill,以模块化方式开发 APICloud App!",
  "main": "index.js",
  "repository": "http://www.apicloud.com",
  "scripts": {
    "serve": "webpack-dev-server --config webpack.dev.config.js",
    "webpack": "webpack",
    "sync": "apicloud wifiSync --project ./ --updateAll false",
    "bundle": "webpack --color --progress",
    "bundle_s": "webpack --color --progress && apicloud wifiSync --project ./ --updateAll false",
    "bundle_d": "webpack -d --color --progress",
    "bundle_d_s": "webpack -d --color --progress && apicloud wifiSync --project ./ --updateAll false"
  },
  "author": "APICloud",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "glob": "^7.1.1",
    "gulp": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "moment": "^2.24.0"
  }
}
